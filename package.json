{
  "name": "@figedi/svc",
  "version": "1.0.0",
  "description": "Yet another base lib for services",
  "repository": "https://github.com/figedi/svc.git",
  "author": "Felix Kaminski <kaminski.felix@gmail.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "build": "npm run clean && tsup",
    "specs": "mocha --exit --extension \"ts\" --node-option \"loader=ts-node/esm\" './src/**/*.spec.ts'",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "test:workerd": "npm run build && wrangler dev test-worker-env.js",
    "test:bun": "npm run build && bun run test-worker-env.js",
    "test:edge-runtime": "npm run clean && bun build src/app/index.ts > dist/index.bun.mjs && node test-edge-runtime-env.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "optionalDependencies": {
    "@kubernetes/client-node": "1.0.0-rc3",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@figedi/sops": "^1.3.7",
    "@figedi/svc-config": "0.1.0",
    "@google-cloud/kms": "^4.0.1",
    "@kubernetes/client-node": "1.0.0-rc3",
    "@tsconfig/node18": "^18.2.1",
    "@types/chai": "^4.3.6",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "@types/node-fetch": "^2.6.2",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chai": "^4.3.7",
    "commitlint": "^17.6.7",
    "edge-runtime": "^2.5.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "nock": "^13.3.2",
    "prettier": "^3.0.3",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "wrangler": "^3.7.0",
    "ws": "^8.14.0"
  },
  "dependencies": {
    "@figedi/metering": "^1.3.1",
    "@figedi/typecop": "^1.3.1",
    "@types/lodash-es": "^4.17.9",
    "@types/semver": "^7.5.1",
    "@types/minimist": "^1.2.2",
    "axios": "^1.4.0",
    "debug": "^4.3.4",
    "envalid": "^7.3.1",
    "fast-json-stable-stringify": "^2.1.0",
    "inversify": "^6.0.1",
    "lodash-es": "^4.17.21",
    "minimist": "^1.2.8",
    "minimist-options": "^4.1.0",
    "pino": "^8.14.1",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "signal-exit": "^4.0.2"
  }
}
